{
  "name": "ibuscloud-ui",
  "version": "2.1.0",
  "description": "React components that implement IBuscloud Design on top of Material-UI.",
  "main": "index.js",
  "module": "es/index.js",
  "scripts": {
    "start": "nodemon --exec yarn run dev",
    "dev:less": "cross-env NODE_ENV=production babel src --out-dir demo/node_modules/ibuscloud-ui --copy-files --ignore *.test.js",
    "dev:es2015": "cross-env NODE_ENV=production babel src -d demo/node_modules/ibuscloud-ui --ignore *.test.js",
    "dev": "yarn dev:less && yarn dev:es2015",
    "prebuild": "rimraf build",
    "build:es2015": "cross-env NODE_ENV=production babel src -d build --ignore *.test.js",
    "build:es": "cross-env NODE_ENV=production BABEL_ENV=es babel src -d build/es --ignore *.test.js",
    "build:copy-files": "babel-node ./script/copy-files.js",
    "build": "yarn prebuild && yarn build:es2015 && yarn build:es && yarn build:copy-files",
    "release": "yarn build && npm publish build",
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "doc": "jsdoc src -r"
  },
  "repository": "git@github.com:marcosun/ibuscloud-ui.git",
  "author": "marcosun <marcoinbristol@gmail.com>",
  "license": "MIT",
  "peerDependencies": {
    "antd": "^3.12.4",
    "less": "^3.9.0",
    "react": ">16.0.0",
    "react-router-dom": "*"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "echarts": "^4.2.0-rc.2",
    "echarts-for-react": "^2.0.15-beta.0",
    "qs": "^6.6.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-do-expressions": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-bind": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.0.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.0.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "antd": "^3.12.4",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.4.2",
    "cross-env": "^5.1.4",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.7.0",
    "fs-extra": "^5.0.0",
    "jest": "^22.4.3",
    "jsdoc": "^3.5.5",
    "nodemon": "^1.17.3",
    "react": "^16.7.0",
    "rimraf": "^2.6.2"
  }
}
