{
  "name": "ibuscloud-ui",
  "version": "1.3.1",
  "description": "React components that implement IBuscloud Design on top of Material-UI.",
  "main": "index.js",
  "module": "es/index.js",
  "scripts": {
    "start": "nodemon --exec yarn run dev",
    "dev": "cross-env NODE_ENV=production babel src -d demo/node_modules/ibuscloud-ui --ignore *.test.js",
    "prebuild": "rimraf build",
    "build:es2015": "cross-env NODE_ENV=production babel src -d build --ignore *.test.js",
    "build:es": "cross-env NODE_ENV=production BABEL_ENV=es babel src -d build/es --ignore *.test.js",
    "build:copy-files": "babel-node ./script/copy-files.js",
    "build": "yarn prebuild && yarn build:es2015 && yarn build:es && yarn build:copy-files",
    "version": "yarn build && npm publish build",
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "doc": "jsdoc src -r"
  },
  "repository": "git@github.com:marcosun/ibuscloud-ui.git",
  "author": "marcosun <marcoinbristol@gmail.com>",
  "license": "MIT",
  "peerDependencies": {
    "@material-ui/icons": "^1.0.0",
    "material-ui": ">1.0.0-beta.39",
    "react": ">16.0.0",
    "react-router-dom": "*"
  },
  "dependencies": {
    "color": "^3.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-jest": "^22.4.3",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "cross-env": "^5.1.4",
    "eslint": "^4.19.1",
    "eslint-config-google": "^0.9.1",
    "eslint-plugin-react": "^7.7.0",
    "fs-extra": "^5.0.0",
    "jest": "^22.4.3",
    "jsdoc": "^3.5.5",
    "nodemon": "^1.17.3",
    "rimraf": "^2.6.2"
  }
}
